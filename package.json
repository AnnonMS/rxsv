{
    "name": "rxsv",
    "version": "0.0.11",
    "description": "RxJS based state management for Vue",
    "author": "Grzegorz Bielski <pesiok@gmail.com>",
    "main": "dist/lib/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/lib/index.d.ts",
    "sideEffects": false,
    "scripts": {
        "build": "npm run clean && tsc -p tsconfig.lib.json && tsc -p tsconfig.esm.json",
        "prepare": "npm run build",
        "test": "jest",
        "test:watch": "jest --watch",
        "clean": "rimraf dist/*",
        "serve:cov": "http-server ./coverage/lcov-report"
    },
    "files": [
        "dist/**/*"
    ],
    "license": "MIT",
    "dependencies": {
        "rxjs": "^6.5.2"
    },
    "devDependencies": {
        "@types/jest": "^24.0.13",
        "fork-ts-checker-webpack-plugin": "^1.3.4",
        "http-server": "^0.11.1",
        "jest": "^24.8.0",
        "prettier": "^1.17.1",
        "rimraf": "^3.0.0",
        "ts-jest": "^24.0.2",
        "ts-loader": "^6.0.2",
        "tslint": "^5.17.0",
        "tslint-config-prettier": "^1.18.0",
        "tslint-immutable": "^5.5.2",
        "tslint-loader": "^3.5.4",
        "typescript": "^3.5.1",
        "webpack": "^4.32.2",
        "webpack-cli": "^3.3.2"
    }
}
